{
  "name": "rekhyt/php-dependency-checker",
  "description": "CLI tool to check dependencies",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Timo Ebel",
      "email": "timo.ebel@gmail.com"
    }
  ],
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "require": {
    "php": ">=5.6",
    "rekhyt/ddd-base": "dev-master@dev",
    "guzzlehttp/guzzle": "~6.0",
    "splitbrain/php-cli": "1.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Rekhyt\\PhpDependencyChecker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rekhyt\\PhpDependencyChecker\\Test\\": "test/"
    }
  },
  "extra": {
    "phar-builder": {
      "compression": "GZip",
      "name": "php-dependency-checker.phar",
      "output-dir": "release",
      "entry-point": "php-dependency-checker",
      "include-dev": false,
      "skip-shebang": false
    }
  }
}
